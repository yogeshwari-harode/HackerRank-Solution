#include<stdio.h>
int main()
{
    int n,arr[100],i;
    float positive=0,negative=0,zero=0;
    
    
    scanf("%d",&n);
    
    for(i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
        
        
        if(arr[i]>0)
            {
                positive++;
               
            }
        else if(arr[i]<0)
            {
                negative++;
                
            }
            else{
                zero++;
            }
    }
    
    printf("%f\n%f\n%f",positive/n,negative/n,zero/n);
        
    return 0;
}
